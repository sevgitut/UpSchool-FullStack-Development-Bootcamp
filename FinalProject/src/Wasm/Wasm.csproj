  </PropertyGroup>

  <ItemGroup>
    <Content Remove="Pages\CrawlerLiveLog.razor" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Blazored.LocalStorage" Version="4.3.0" />
