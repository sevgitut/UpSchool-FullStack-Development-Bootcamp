  </ItemGroup>

  <ItemGroup>
    <UpToDateCheckInput Remove="Pages\CrawlerLiveLog.razor" />
  </ItemGroup>

  <ItemGroup>
    <_ContentIncludedByDefault Remove="Pages\CrawlerLiveLog.razor" />
